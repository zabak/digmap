<?xml version="1.0"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

<display-name>Google Web 1T N-Gram Index</display-name>
 <servlet>
   <servlet-name>SearchNGramsServlet</servlet-name>
   <description>SearchNGramsServlet</description>
   <servlet-class>SearchNGramsServlet</servlet-class>
 </servlet>
 <servlet-mapping>
   <servlet-name>SearchNGramsServlet</servlet-name>
   <url-pattern>/</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
   <servlet-name>OAIHandler</servlet-name>
   <url-pattern>/ngram</url-pattern>
 </servlet-mapping>
</web-app>